   <h1 class="text-center my-5">Productos a√±adidos!!</h1>
 <% object.forEach(object => { %>
        <table class="table table-bordered table-striped text-center mt-4" >
         <thead>
            <tr>
              <th scope="col">Nombre: <%= object.name %></th>
              <th scope="col">Precio: <%= object.precio %></th>
              <th scope="col">Color: <%= object.color %></th>
              <th scope="col"> Imagen: <img scope="row" src="http://localhost:8080/img/<%= object.image %>"></th>
            </tr>
         </thead>
         <% }) %>
          <tbody>
        </table>

        <div class="mx-auto text-center">
          <a class="btn btn-dark mx-auto text-center my-5" href="/newObject">Ir a crear productos</a>
       </div>      
    